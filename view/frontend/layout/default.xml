<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="top.search">
            <block ifconfig="coditron_searchautocomplete/general/enable" class="Coditron\SearchAutoComplete\Block\SearchAutoComplete" name="searchautocomplete.list" as="searchautocomplete_list" before="-" template="Coditron_SearchAutoComplete::searchautocomplete.phtml" />
            <block ifconfig="coditron_searchautocomplete/general/enable" class="Coditron\SearchAutoComplete\Block\SearchAutoComplete" name="searchautocomplete.results" as="results" after="-" template="Coditron_SearchAutoComplete::results.phtml" />
        </referenceBlock>
    </body>
</page>
